{
    "manifest_version": 2,

    "name": "Font Clipper",
    "description": "Store web font name and source link from web page",
    "version": "1.0",

    "permissions": [
        "fontSettings",
        "notifications"
    ],

    "browser_action": {
        // "default_icon": "public/images/icon.png",
        "default_popup": "public/html/popup.html"
    }
}